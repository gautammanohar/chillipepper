# Define the directory where cron script will be placed.
logcount_dir: '/logcount_dir'

# Define the name of the cron script. This will be concatanated
# with the dir name in the state file.
logcount_script: 'logcount_script.sh'

# Define the full path of the log file
# where counts will be appended every 30 minutes.
count_file: '/root/counts.log'
